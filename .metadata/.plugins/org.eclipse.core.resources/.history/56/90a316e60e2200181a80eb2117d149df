package Datum20180307;

public class GeenGeluid
{
	public static void main(String[] args)
	{
		GeenGeluid gg = new GeenGeluid();
		gg.start();
	}
		void start()
		{
		Versterker versterker = new Versterker();
		versterker.aan = true;

		Spanning spanning = new Spanning();
		try
			{
				versterker.spanning = false;
			}
		catch (Exception e)
			{
				System.out.println("de kabel zit er niet in");
			}
		}	
	
}
class Versterker
{
	boolean aan = true;
	boolean spanning = true;
}
class Spanning
{
	boolean geluid = true;
}
